    For iNum = 1 To UBound(iAmount, 1)
        
        For x = 58 To 87
        
            If Cells(x, 2).Value = iAmount(iNum, 1) Then
            
                Cells(x, 2).Offset(0, 3).Value = iAmount(iNum, 16)
            
            Else
            
            End If
            
        Next x
    
    Next iNum
    
'MOD 767
    
bp.Sheets("767_Input").Activate

    For iNum = 1 To UBound(iAmount, 1)
        
        For x = 58 To 87
        
            If Cells(x, 2).Value = iAmount(iNum, 1) Then
            
