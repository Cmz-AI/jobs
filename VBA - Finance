Attribute VB_Name = "Module2"
Sub Faster()

Application.ScreenUpdating = False
Application.Calculation = xlCalculationManual
Application.EnableEvents = False
Application.DisplayStatusBar = False

End Sub

Sub Backup()

Application.DisplayAlerts = False

ActiveWorkbook.SaveAs Filename:="C:\Users\ap369e\Desktop\2023 Business Plan BGS_C Backup.xlsm"

Application.DisplayAlerts = True

End Sub
Sub Year23()

Dim x As Long
Dim iAmount() As Variant
Dim iNum As Integer
Dim iAmount2() As Variant
Dim iNum2 As Integer
Dim ba As Workbook
Dim bp As Workbook

'Set Workbooks
Set ba = Workbooks("Pivot_BGS-C_Source_Business Plan Actuals.xlsx")
Set bp = Workbooks("2023 Business Plan BGS_C.xlsm")


'Prepare Approriate Data by setting Pivot
ba.Sheets("Business_Plan-by program").Activate
    
    With ActiveSheet.PivotTables("PivotTable4").PivotFields("YEAR")
        .PivotItems("2023").Visible = True
        .PivotItems("2022").Visible = False
        .PivotItems("(blank)").Visible = False
    End With
    
    With ActiveSheet.PivotTables("PivotTable4").PivotFields("Month")
        .PivotItems("JAN").Visible = True
        .PivotItems("FEB").Visible = False
        .PivotItems("MAR").Visible = False
        .PivotItems("APR").Visible = False
        .PivotItems("MAY").Visible = False
        .PivotItems("JUN").Visible = False
        .PivotItems("JUL").Visible = False
        .PivotItems("AUG").Visible = False
        .PivotItems("SEP").Visible = False
        .PivotItems("OCT").Visible = False
        .PivotItems("NOV").Visible = False
        .PivotItems("DEC").Visible = False
        .PivotItems("(blank)").Visible = False
    End With

'Array Creation
iAmount = Range("A13:AL43")

'************************************************JAN23***********************************************************************************************************

'MOD APS
bp.Sheets("Mods_APS_Input").Activate

    For iNum = 1 To UBound(iAmount, 1)
        
        For x = 58 To 87
        
            If Cells(x, 2).Value = iAmount(iNum, 1) Then
            
                Cells(x, 2).Offset(0, 3).Value = iAmount(iNum, 2)
            
            Else
            
            End If
            
        Next x
    
    Next iNum
    
'MODS AVIONICS
bp.Sheets("Mods_Avionics__Input").Activate

    For iNum = 1 To UBound(iAmount, 1)
        
        For x = 58 To 87
        
            If Cells(x, 2).Value = iAmount(iNum, 1) Then
            
                Cells(x, 2).Offset(0, 3).Value = iAmount(iNum, 4)
            
            Else
            
            End If
            
        Next x
    
    Next iNum

'MODS Connectivity
bp.Sheets("Mods_Connectivity_Input").Activate

    For iNum = 1 To UBound(iAmount, 1)
        
        For x = 58 To 87
        
            If Cells(x, 2).Value = iAmount(iNum, 1) Then
            
                Cells(x, 2).Offset(0, 3).Value = iAmount(iNum, 6)
            
            Else
            
            End If
            
        Next x
    
    Next iNum
    
'MOD Interiors
bp.Sheets("Mods_Interiors_Input").Activate

    For iNum = 1 To UBound(iAmount, 1)
        
        For x = 58 To 95
        
            If Cells(x, 2).Value = iAmount(iNum, 1) Then
            
                Cells(x, 2).Offset(0, 3).Value = iAmount(iNum, 8)
            
            Else
            
            End If
            
        Next x
    
    Next iNum
    
'MOD Others
bp.Sheets("ModOther_Input").Activate

    For iNum = 1 To UBound(iAmount, 1)
        
        For x = 58 To 87
        
            If Cells(x, 2).Value = iAmount(iNum, 1) Then
            
                Cells(x, 2).Offset(0, 3).Value = iAmount(iNum, 10)
            
            Else
            
            End If
            
        Next x
    
    Next iNum
    
    
'MOD 737BCF

bp.Sheets("737BCF_Input").Activate

    For iNum = 1 To UBound(iAmount, 1)
        
        For x = 58 To 87
        
            If Cells(x, 2).Value = iAmount(iNum, 1) Then
            
                Cells(x, 2).Offset(0, 3).Value = iAmount(iNum, 12)
            
            Else
            
            End If
            
        Next x
    
    Next iNum
    
'MOD 767BCF
    
bp.Sheets("767BCF_Input").Activate

    For iNum = 1 To UBound(iAmount, 1)
        
        For x = 58 To 87
        
            If Cells(x, 2).Value = iAmount(iNum, 1) Then
            
                Cells(x, 2).Offset(0, 3).Value = iAmount(iNum, 14)
            
            Else
            
            End If
            
        Next x
    
    Next iNum
    
'MOD 747
    
