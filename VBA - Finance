    End With
    
    With ActiveSheet.PivotTables("PivotTable4").PivotFields("Month")
        .PivotItems("JAN").Visible = True
        .PivotItems("FEB").Visible = True
        .PivotItems("MAR").Visible = True
        .PivotItems("APR").Visible = True
        .PivotItems("MAY").Visible = True
        .PivotItems("JUN").Visible = True
        .PivotItems("JUL").Visible = True
        .PivotItems("AUG").Visible = True
        .PivotItems("SEP").Visible = True
        .PivotItems("OCT").Visible = True
        .PivotItems("NOV").Visible = True
        .PivotItems("DEC").Visible = True
        .PivotItems("(blank)").Visible = False
    End With

iAmount = Range("A11:N30")

bp.Sheets("OOP_CS_Input").Activate

    For iNum = 1 To UBound(iAmount, 1)
        
        For x = 58 To 87
        
            If Cells(x, 2).Value = iAmount(iNum, 1) Then
            
                Cells(x, 2).Offset(0, 3).Value = iAmount(iNum, 2)
                Cells(x, 2).Offset(0, 4).Value = iAmount(iNum, 3)
                Cells(x, 2).Offset(0, 5).Value = iAmount(iNum, 4)
                Cells(x, 2).Offset(0, 6).Value = iAmount(iNum, 5)
                Cells(x, 2).Offset(0, 7).Value = iAmount(iNum, 6)
                Cells(x, 2).Offset(0, 8).Value = iAmount(iNum, 7)
                Cells(x, 2).Offset(0, 9).Value = iAmount(iNum, 8)
                Cells(x, 2).Offset(0, 10).Value = iAmount(iNum, 9)
                Cells(x, 2).Offset(0, 11).Value = iAmount(iNum, 10)
                Cells(x, 2).Offset(0, 12).Value = iAmount(iNum, 11)
                Cells(x, 2).Offset(0, 13).Value = iAmount(iNum, 12)
                Cells(x, 2).Offset(0, 14).Value = iAmount(iNum, 13)
                
            Else
            
            End If
            
        Next x
    
    Next iNum

End Sub
