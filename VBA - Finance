Sub Others()

Dim x As Long
Dim iAmount() As Variant
Dim iNum As Integer
Dim iAmount2() As Variant
Dim iNum2 As Integer
Dim ba As Workbook
Dim bp As Workbook

Set ba = Workbooks("Pivot_BGS-C_Source_Business Plan Actuals.xlsx")
Set bp = Workbooks("2023 Business Plan BGS_C.xlsm")

'********************************************************************IP CS**********************************************************************************************

ba.Sheets("Business_Plan-by program").Activate

    With ActiveSheet.PivotTables("PivotTable4").PivotFields("YEAR")
        .PivotItems("2023").Visible = True
        .PivotItems("2022").Visible = False
        .PivotItems("(blank)").Visible = False
    End With
    
    With ActiveSheet.PivotTables("PivotTable4").PivotFields("BUSINESS PLAN")
        
        .PivotItems("IP CS").Visible = True
        .PivotItems("MODS AP&S").Visible = False
        .PivotItems("MODS AVIONICS").Visible = False
        .PivotItems("MODS CONNECTIVITY").Visible = False
        .PivotItems("MODS INTERIORS").Visible = False
        .PivotItems("MOD OTHER").Visible = False
        .PivotItems("BCF737").Visible = False
        .PivotItems("BCF767").Visible = False
        .PivotItems("747").Visible = False
        .PivotItems("767").Visible = False
        .PivotItems("787").Visible = False
        .PivotItems("").Visible = False
        .PivotItems("737NG / MAX").Visible = False
        .PivotItems("777 / 777X").Visible = False
        .PivotItems("BGS OTHER").Visible = False
        .PivotItems("BGS SUB-COMPONENTS").Visible = False
        .PivotItems("MODS BAGA").Visible = False
        .PivotItems("OFFLOADS & PD").Visible = False
        .PivotItems("OOP CS").Visible = False
        .PivotItems("OVERHEAD").Visible = False
        .PivotItems("PARTS DESIGN").Visible = False
        .PivotItems("(blank)").Visible = False
        .PivotItems("SALES SUPPORT").Visible = False
    End With
    
    With ActiveSheet.PivotTables("PivotTable4").PivotFields("Month")
        .PivotItems("JAN").Visible = True
        .PivotItems("FEB").Visible = True
        .PivotItems("MAR").Visible = True
        .PivotItems("APR").Visible = True
        .PivotItems("MAY").Visible = True
        .PivotItems("JUN").Visible = True
        .PivotItems("JUL").Visible = True
        .PivotItems("AUG").Visible = True
        .PivotItems("SEP").Visible = True
        .PivotItems("OCT").Visible = True
        .PivotItems("NOV").Visible = True
        .PivotItems("DEC").Visible = True
        .PivotItems("(blank)").Visible = False
    End With

iAmount = Range("A11:N30")

bp.Sheets("IP_CS_Input").Activate

    For iNum = 1 To UBound(iAmount, 1)
        
        For x = 58 To 87
        
            If Cells(x, 2).Value = iAmount(iNum, 1) Then
            
                Cells(x, 2).Offset(0, 3).Value = iAmount(iNum, 2)
                Cells(x, 2).Offset(0, 4).Value = iAmount(iNum, 3)
                Cells(x, 2).Offset(0, 5).Value = iAmount(iNum, 4)
                Cells(x, 2).Offset(0, 6).Value = iAmount(iNum, 5)
                Cells(x, 2).Offset(0, 7).Value = iAmount(iNum, 6)
                Cells(x, 2).Offset(0, 8).Value = iAmount(iNum, 7)
                Cells(x, 2).Offset(0, 9).Value = iAmount(iNum, 8)
                Cells(x, 2).Offset(0, 10).Value = iAmount(iNum, 9)
                Cells(x, 2).Offset(0, 11).Value = iAmount(iNum, 10)
                Cells(x, 2).Offset(0, 12).Value = iAmount(iNum, 11)
                Cells(x, 2).Offset(0, 13).Value = iAmount(iNum, 12)
                Cells(x, 2).Offset(0, 14).Value = iAmount(iNum, 13)
            
            Else
            
            End If
            
        Next x
    
    Next iNum
    
'********************************************************************IP CS**********************************************************************************************

ba.Sheets("Business_Plan-by program").Activate

    With ActiveSheet.PivotTables("PivotTable4").PivotFields("YEAR")
        .PivotItems("2023").Visible = True
        .PivotItems("2022").Visible = False
        .PivotItems("(blank)").Visible = False
    End With
    
    With ActiveSheet.PivotTables("PivotTable4").PivotFields("BUSINESS PLAN")
        
        .PivotItems("OOP CS").Visible = True
        .PivotItems("IP CS").Visible = False
        .PivotItems("MODS AP&S").Visible = False
        .PivotItems("MODS AVIONICS").Visible = False
        .PivotItems("MODS CONNECTIVITY").Visible = False
        .PivotItems("MODS INTERIORS").Visible = False
        .PivotItems("MOD OTHER").Visible = False
        .PivotItems("BCF737").Visible = False
        .PivotItems("BCF767").Visible = False
        .PivotItems("747").Visible = False
        .PivotItems("767").Visible = False
        .PivotItems("787").Visible = False
        .PivotItems("").Visible = False
        .PivotItems("737NG / MAX").Visible = False
        .PivotItems("777 / 777X").Visible = False
        .PivotItems("BGS OTHER").Visible = False
        .PivotItems("BGS SUB-COMPONENTS").Visible = False
        .PivotItems("MODS BAGA").Visible = False
        .PivotItems("OFFLOADS & PD").Visible = False
        .PivotItems("OVERHEAD").Visible = False
        .PivotItems("PARTS DESIGN").Visible = False
        .PivotItems("(blank)").Visible = False
