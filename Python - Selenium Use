from selenium import webdriver
from selenium.webdriver.common.by import By

webpage = r"XXX" # edit website
searchterm = "!!!!!!!" # My ID

driver = webdriver.Firefox()
driver.get(webpage)

sbox = driver.find_element(By.ID, 'bemstxt')
sbox.send_keys(searchterm)

sbox = driver.find_element(By.ID, 'AttndIN').click()

